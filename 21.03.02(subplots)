fig, axs = plt.subplots(3, 4, figsize=(15, 10))

axs[0, 0].hist(cont.iloc[:,0], bins = 1000)
axs[0, 0].set_title("cont0")

axs[0, 1].hist(cont.iloc[:,1], bins = 1000)
axs[0, 1].set_title("cont1")

axs[0, 2].hist(cont.iloc[:,2], bins = 1000)
axs[0, 2].set_title("cont2")

axs[0, 3].hist(cont.iloc[:,3], bins = 1000)
axs[0, 3].set_title("cont3")

axs[1, 0].hist(cont.iloc[:,4], bins = 1000)
axs[1, 0].set_title("cont4")

axs[1, 1].hist(cont.iloc[:,5], bins = 1000)
axs[1, 1].set_title("cont5")

axs[1, 2].hist(cont.iloc[:,6], bins = 1000)
axs[1, 2].set_title("cont6")

axs[1, 3].hist(cont.iloc[:,7], bins = 1000)
axs[1, 3].set_title("cont7")

axs[2, 0].hist(cont.iloc[:,8], bins = 1000)
axs[2, 0].set_title("cont8")

axs[2, 1].hist(cont.iloc[:,9], bins = 1000)
axs[2, 1].set_title("cont9")

axs[2, 2].hist(cont.iloc[:,10], bins = 1000)
axs[2, 2].set_title("cont10")



fig.tight_layout()
