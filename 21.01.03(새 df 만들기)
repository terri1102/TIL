##새로운 데이터 프레임 만들기
df1 = pd.DataFrame({'a': ['123', '2334', '234', '3445', '545'],
                    'b': ['도레미', '파솔라', '시', '도', '레']})                         #대괄호, 중괄호,  신경쓰기!
 
table1 = pd.DataFrame(
    [[np.nan, 2],
     [16,    11], 
     [3,      1]],
    index=['X', 'Y', 'Z'],                                                                #index와 column 값을 직접 지정 가능
    columns=['A', 'B'])
